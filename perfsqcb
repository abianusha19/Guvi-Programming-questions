import java.util.*;
import java.lang.Math;
public class perfsqcb
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int i=n;
		while(true)
		{
		    double sq=Math.sqrt(i);
		    double cb=Math.cbrt(i);
		    if(sq==(int)sq && cb==(int)cb)
		    {
		        System.out.println(i);
		        break;
		    }
		    i--;
		}
	}
}
